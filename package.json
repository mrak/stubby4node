{
  "name": "stubby",
  "version": "4.0.0",
  "author": {
    "name": "Eric Mrak",
    "email": "mail@ericmrak.info"
  },
  "bugs": {
    "url": "https://github.com/mrak/stubby4node"
  },
  "description": "a lightweight server for stubbing external systems and endpoints",
  "keywords": [
    "server",
    "stub",
    "mock",
    "testing",
    "service",
    "endpoint",
    "http",
    "https",
    "api",
    "rest"
  ],
  "homepage": "https://github.com/mrak/stubby4node",
  "contributors": [
    {
      "name": "Eric Mrak",
      "email": "mail@ericmrak.info"
    }
  ],
  "directories": {
    "bin": "./bin",
    "lib": "./src",
    "man": "./man"
  },
  "files": [
    "bin",
    "man",
    "src",
    "tls",
    "webroot"
  ],
  "scripts": {
    "lint": "semistandard",
    "test": "npm run lint && mocha --recursive test --reporter dot",
    "start": "bin/stubby"
  },
  "bin": {
    "stubby": "bin/stubby"
  },
  "main": "src/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrak/stubby4node.git"
  },
  "dependencies": {
    "async": "^0.2.7",
    "ejs": "^0.8.4",
    "isutf8": "^1.0.11",
    "js-yaml": "^1.0.1",
    "node-static": "^0.6.4"
  },
  "devDependencies": {
    "buffer-equal": "^1.0.0",
    "mocha": "^3.1.2",
    "semistandard": "^9.2.1",
    "sinon": "^1.17.6"
  },
  "license": "Apache-2.0",
  "engine": {
    "node": ">=4.0.0"
  },
  "semistandard": {
    "ignore": [
      "webroot/js/external"
    ],
    "globals": [
      "it",
      "describe",
      "beforeEach",
      "afterEach"
    ]
  }
}
